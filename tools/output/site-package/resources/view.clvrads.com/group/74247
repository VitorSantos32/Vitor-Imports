		$(window).on("load", function() {
			var $boxes = $("div.box");
			
			if ($boxes.length) {
				var $shape = $('<div id="shape" class="cube backfaces"></div>');
				$boxes.wrapAll($shape);
			}
			
			var $banners = $("div[class^='banner']");
			
			if ($banners.length) {
				var $shape = $('<div id="shape" class="cube backfaces"></div>');
				$banners.wrapAll($shape);
			}
			
			var $shapeDiv = $("#shape");
			if ($shapeDiv.length) {
				var $stage = $('<div id="stage"></div>');
				$shapeDiv.wrapAll($stage);
			}
			
			$("#shape > div").each(function(index) {
				var classNames = ["plane one", "plane two", "plane three", "plane four"];
				if (index < classNames.length) {
					$(this).addClass(classNames[index]);
				}
			});
			
			$(".box").removeClass("banner1 banner2 banner3 banner4");
			
		});
	</script>
